  "bugs": {
    "url": "https://github.com/elvinn/leetcode/issues"
  },
  "homepage": "https://github.com/elvinn/leetcode#readme",
  "devDependencies": {
    "ava": "^2.3.0",
    "fs-extra": "^8.1.0",
    "prettier-standard": "^14.0.5"
  },
  "ava": {
    "files": [
      "src/**/*"
